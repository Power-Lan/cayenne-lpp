{
    "name": "power-lan/cayenne-lpp",
    "type": "library",
    "description": "PHP Cayenne LPP encoder and decoder",
    "keywords": [
        "cayennelpp",
        "cayenne",
        "lpp",
        "ipso",
        "lora",
        "lorawan",
        "sigfox"
    ],
    "homepage": "http://www.photon-project.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "William MARTIN",
            "email": "william.martin@power-lan.com",
            "role": "Developer"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/Power-Lan/cayenne-lpp/blob/master/CONTRIBUTORS.md"
        }
    ],
    "require": {
        "php" : "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "CayenneLPP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CayenneLPP\\Test\\": "tests/"
        }
    },
    "scripts": {
      "phpunit": "phpunit",
      "phpcs": "phpcs --standard=PSR2 src tests",
      "phpcbf": "phpcbf --standard=PSR2 src tests"
    }
}
